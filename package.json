{
  "name": "redux-counter-example",
  "version": "0.1.0",
  "description": "Redux  example app for the redradix baseapp",
  "scripts": {
    "start": "npm run clean && NODE_ENV=development nodemon ./src/server.js -w ./src/server.js",
    "start-prod": "NODE_ENV=production node ./src/server.js",
    "build": "NODE_ENV=production webpack -p --progress --colors --config ./config/webpack.config.prod",
    "test": "NODE_ENV=test mocha --recursive src/test --compilers js:babel-core/register --require ./src/test/setup.js",
    "test:modules": "NODE_ENV=test mocha --recursive ./src/modules/ --compilers js:babel-core/register --require ./src/test/setup.js",
    "test:watch": "npm test -- --watch",
    "lint": "eslint ./src/**",
    "clean": "rm -f public/index.html && rm -rf public/js"
  },
  "engines": {
    "node": ">=4.2.6",
    "npm": ">=2.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redradix/reduxBaseApp.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redradix/reduxBaseAp/issues"
  },
  "homepage": "http://redradix.com",
  "dependencies": {
    "dom-scroll-into-view": "^1.0.1",
    "history": "^2.0.0-rc2",
    "i18next": "^2.0.23",
    "i18next-browser-languagedetector": "0.0.14",
    "i18next-xhr-backend": "^0.3.0",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.7",
    "react-autocomplete": "^0.1.4",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^0.14.7",
    "react-i18next": "^0.1.0",
    "react-modal": "^0.6.1",
    "react-motion": "^0.4.2",
    "react-redux": "^4.1.2",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "react-router-transition": "0.0.4",
    "redux": "^3.1.6",
    "redux-devtools": "^3.0.0",
    "redux-form": "^4.1.6",
    "redux-thunk": "^1.0.3",
    "reselect": "^2.0.2",
    "superagent": "^1.7.2",
    "moment": "^2.11.1",
    "moment-range": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "css-loader": "^0.23.1",
    "expect": "^1.6.0",
    "express": "^4.13.3",
    "fetch-mock": "^4.1.1",
    "html-webpack-plugin": "^2.7.1",
    "jquery": "^2.2.0",
    "jsdom": "^8.0.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "nodemon": "^1.8.1",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-hmr": "^1.0.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
